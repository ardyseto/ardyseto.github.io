  html={https://iopscience.iop.org/article/10.1088/1742-6596/2111/1/012046/pdf},
  preview={priambodo_face_2021.png},
  pdf={Priambodo_2021_J._Phys.__Conf._Ser._2111_012046.pdf},
  google_scholar_id={Tyk-4Ss8FVUC},
